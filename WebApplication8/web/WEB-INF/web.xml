<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>crt</servlet-name>
        <servlet-class>abc.crt</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crt1</servlet-name>
        <servlet-class>abc.crt1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crt2</servlet-name>
        <servlet-class>abc.crt2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crt3</servlet-name>
        <servlet-class>crt3</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>crt4</servlet-name>
        <servlet-class>abc.crt4</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>remove</servlet-name>
        <servlet-class>abc.remove</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>crt</servlet-name>
        <url-pattern>/crt</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crt1</servlet-name>
        <url-pattern>/crt1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crt2</servlet-name>
        <url-pattern>/crt2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crt3</servlet-name>
        <url-pattern>/crt3</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>crt4</servlet-name>
        <url-pattern>/crt4</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>remove</servlet-name>
        <url-pattern>/remove</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
